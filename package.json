    "babel-loader": "^8.2.4",
    "eslint": "^8.1.0",
    "eslint-plugin-jest": "^25.2.3",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.71.0",
